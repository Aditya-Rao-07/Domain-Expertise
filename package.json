{
  "name": "wordpress-site-analyzer",
  "version": "2.0.0",
  "description": "A tool to analyze WordPress sites without admin access, detecting version, theme, and plugins",
  "main": "index.js",
  "bin": {
    "wp-analyze": "./cli.js"
  },
  "scripts": {
    "start": "node cli.js",
    "analyze": "node cli.js",
    "analyze-html": "node cli.js --html",
    "quick-test": "node -e \"const WPA = require('./index'); new WPA().quickDetect('wordpress.org').then(r => console.log(r))\"",
    "web": "node server.js",
    "dev": "node server.js"
  },
  "keywords": [
    "wordpress",
    "security",
    "analysis",
    "reconnaissance",
    "plugins",
    "themes",
    "version-detection"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/wordpress-site-analyzer.git"
  }
}
